extends layout

block content
    div(id="viz")
        canvas(id="analyser" width="1024" height="500")
        canvas(id="wavedisplay" width="1024" height="500")
    div(id="controls")
        img(id="record" src="data:image/png;base64," onclick="toggleRecording(this);")
        div
            input(type="text" id="ttsText")
            button(onclick="getSpeechFromText(this.previousSibling);") Send
        a(id="save" href="#")
            img(src="data:image/svg+xml;base64,")
    a(href="/" style="height: 50px;") Zurck zur Konversation
    audio(id="audioPlayer" src="" controls style="display:none")

    script(src='/javascripts/audiodisplay.js')
    script(src='/javascripts/recorderjs/recorder.js')
    script(src='/javascripts/audioMain.js')